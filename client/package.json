{
    "name": "guliguli",
    "main": ".webpack/main",
    "version": "1.0.0",
    "scripts": {
        "start": "electron-forge start",
        "package": "electron-forge package",
        "make": "electron-forge make --platform=win32 --arch=x64",
        "publish": "electron-forge publish",
        "lint": "eslint . --cache --ext .js,.jsx,.ts,.tsx --max-warnings 0",
        "prepare": "husky install",
        "build": "node webpack.cicd.config.js"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "./**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
            "prettier --write",
            "yarn lint"
        ]
    },
    "config": {
        "forge": {
            "packagerConfig": {
                "icon": "./icon.ico"
            },
            "makers": [
                {
                    "name": "@electron-forge/maker-squirrel",
                    "config": {
                        "name": "login"
                    }
                },
                {
                    "name": "@electron-forge/maker-zip",
                    "platforms": [
                        "darwin"
                    ]
                },
                {
                    "name": "@electron-forge/maker-deb",
                    "config": {}
                },
                {
                    "name": "@electron-forge/maker-rpm",
                    "config": {}
                }
            ],
            "plugins": [
                {
                    "name": "@electron-forge/plugin-webpack",
                    "config": {
                        "mainConfig": "./webpack.main.config.js",
                        "renderer": {
                            "config": "./webpack.renderer.config.js",
                            "entryPoints": [
                                {
                                    "html": "./src/index.html",
                                    "js": "./src/renderer.tsx",
                                    "name": "main_window"
                                }
                            ]
                        },
                        "devServer": {
                            "liveReload": false,
                            "devMiddleware": {
                                "writeToDisk": false
                            }
                        },
                        "devContentSecurityPolicy": "style-src 'self' 'unsafe-inline'; connect-src * data: blob: 'unsafe-inline'; img-src * 'self' data: blob: 'unsafe-inline';",
                        "port": 3000,
                        "loggerPort": 6789
                    }
                }
            ]
        }
    },
    "devDependencies": {
        "@electron-forge/cli": "^7.8.1",
        "@electron-forge/maker-deb": "^7.8.1",
        "@electron-forge/maker-rpm": "^7.8.1",
        "@electron-forge/maker-squirrel": "^7.8.1",
        "@electron-forge/maker-zip": "^7.8.1",
        "@electron-forge/plugin-webpack": "^7.8.1",
        "@pmmmwh/react-refresh-webpack-plugin": "^0.6.1",
        "@reduxjs/toolkit": "^2.8.2",
        "@types/fs-extra": "^11.0.4",
        "@types/qrcode.react": "^3.0.0",
        "@types/react": "^19.1.8",
        "@types/react-dom": "^19.1.6",
        "@types/react-router": "^5.1.20",
        "@types/react-router-dom": "^5.3.3",
        "@typescript-eslint/eslint-plugin": "^8.35.0",
        "@typescript-eslint/parser": "^8.35.0",
        "@vercel/webpack-asset-relocator-loader": "1.7.4",
        "axios": "^1.10.0",
        "css-loader": "^7.1.2",
        "electron": "37.1.0",
        "electron-squirrel-startup": "^1.0.1",
        "eslint": "^9.30.0",
        "eslint-import-resolver-alias": "^1.1.2",
        "eslint-import-resolver-typescript": "^4.4.4",
        "eslint-plugin-import": "^2.32.0",
        "fork-ts-checker-webpack-plugin": "^9.1.0",
        "husky": "^9.1.7",
        "lint-staged": "^16.1.2",
        "node-loader": "^2.1.0",
        "qrcode.react": "^4.2.0",
        "react-refresh": "^0.17.0",
        "react-refresh-typescript": "^2.0.10",
        "react-router": "^7.6.3",
        "react-router-dom": "^7.6.3",
        "react-star-ratings": "^2.3.0",
        "style-loader": "^4.0.0",
        "ts-loader": "^9.5.2",
        "typescript": "^5.8.3",
        "xlsx": "^0.18.5"
    },
    "dependencies": {
        "@emotion/react": "^11.14.0",
        "@emotion/styled": "^11.14.1",
        "@mui/icons-material": "^7.1.2",
        "@mui/lab": "^7.0.0-beta.14",
        "@mui/material": "^7.1.2",
        "@mui/system": "^7.1.1",
        "@mui/x-data-grid": "^8.6.0",
        "@react-pdf/renderer": "^4.3.0",
        "@types/crypto-js": "^4.2.2",
        "@types/lodash": "^4.17.19",
        "@types/pako": "^2.0.3",
        "@types/qrcode": "^1.5.5",
        "antd": "^5.26.2",
        "class-transformer": "^0.5.1",
        "compressing": "^1.10.3",
        "copy-webpack-plugin": "^13.0.0",
        "crypto-js": "^4.2.0",
        "date-fns": "^4.1.0",
        "element-react": "^1.4.34",
        "element-theme-default": "^1.4.13",
        "eslint-import-resolver-alias": "^1.1.2",
        "eslint-import-resolver-typescript": "^4.4.4",
        "file-loader": "^6.2.0",
        "fs": "^0.0.1-security",
        "fs-extra": "^11.3.0",
        "hls.js": "^1.6.6",
        "idb-keyval": "^6.2.2",
        "js-base64": "^3.7.7",
        "jsonfile": "^6.1.0",
        "lodash": "^4.17.21",
        "madge": "^8.0.0",
        "minio": "^8.0.5",
        "moment": "^2.30.1",
        "pako": "^2.1.0",
        "path-browserify": "^1.0.1",
        "pinyin": "^4.0.0",
        "prettier": "^3.6.2",
        "qrcode": "^1.5.4",
        "react": "^19.1.0",
        "react-dom": "^19.1.0",
        "react-draggable": "^4.5.0",
        "redux-persist": "^6.0.0",
        "reflect-metadata": "^0.2.2",
        "slash": "^5.1.0",
        "terser-webpack-plugin": "^5.3.14",
        "zustand": "^5.0.6"
    }
}
